      Scanner sc = new Scanner(System.in);
      String a = sc.next();
      System.out.println("Hi "+a+", welcome to the programming class!");
  }
}

//11(Calculate Total and average Marks)
import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner sc = new Scanner(System.in);
      String a = sc.next();
      int b = sc.nextInt();
      int c = sc.nextInt();
      int d = sc.nextInt();
      int e = sc.nextInt();
      int f = sc.nextInt();
      int total = b + c + d + e + f;
      double average = total / 5.0;
      System.out.println("Student Name: " +a);
      System.out.println("Total Marks: " +total);
      System.out.printf("Average Marks: %.2f\n",average);
  }
}

//12(Shape Area Calculation)
import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner obj = new Scanner(System.in);
      int a = obj.nextInt();
      int l = obj.nextInt();
      int b = obj.nextInt();
      int r = obj.nextInt();
      int B = obj.nextInt();
      int h = obj.nextInt();
      double pi=3.14;
      System.out.println("Area of Square="+(a*a));
      System.out.println("Area of Rectangle="+(l*b));
      System.out.printf("Area of Circle=%.2f\n",pi*Math.pow(r,2));
      System.out.println("Area of Triangle="+(int)((0.5)*B*h));
    }
}

//13(Shape Permiter Calculation)
import java.util.*;

class ShapePerimeter {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        
        int a = sc.nextInt();   
        int l = sc.nextInt();   
        int w = sc.nextInt();   
        int r = sc.nextInt();   
        int b1 = sc.nextInt();  
        int s = sc.nextInt();   
        int b2 = sc.nextInt();  

        int square = 4 * a;
        int rectangle = 2 * (l + w);
        double circle = 2 * 3.14 * r;
        int triangle = b1 + s + b2;

        System.out.println(square);
        System.out.println(rectangle);
        System.out.printf("%.2f\n", circle);
        System.out.println(triangle);
    }
}

//13(Handling mixed Inputs in Java)
import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner obj = new Scanner(System.in);
      int a = obj.nextInt();
      obj.nextLine();
      String s = obj.nextLine();
      System.out.println(a);
      System.out.println(s);
  }
}
