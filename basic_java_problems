        int a = sc.nextInt();
        System.out.println((char)a);

  }
}

//6(Update Counter)
import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner sc = new Scanner(System.in);
      int a = sc.nextInt();
      System.out.println(((a+5)*2)-3);

  }
}

//7(ODD OR EVEN)
import java.util.*;

public class Main {
    public static void main(String[] args) {
     Scanner sc = new Scanner(System.in);
     int a = sc.nextInt();
     if(a%2==0)
      System.out.println("Even");
     else
     System.out.println("Odd");
  }
}

//8(Greatest among three numbers)
import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner sc = new Scanner(System.in);
      int a = sc.nextInt();
      int b = sc.nextInt();
      int c = sc.nextInt();
      if (a>b && a>c)
      System.out.println(a);
      else if (b>a && b>c)
      System.out.println(b);
      else
      System.out.println(c);
  }
}

//9(Character digit to Integer)
import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner sc = new Scanner(System.in);
      char a = sc.next().charAt(0);

      System.out.println((int)a-48);
  }
}

